#include<iostream>
using namespace std;

//Note: there is no change by call by value ,see result
//it copy value only
void swap(int x, int y)
{
	int temp=x;
	x=y;
	y=temp;
	return ;
}
 int main()
 {
    int a=200,b=500;
	cout<<"value of a before swap"<<a <<endl;
	cout<<"value of b before swap"<<b <<endl;
	swap(a,b);
	cout<<"value of a  after swap"<<a<<endl;
	cout<<"value of b after swap"<<a<<endl;
	return 0;
 }
